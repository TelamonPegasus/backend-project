{
    "name": "a2ensite/octo",
    "type": "project",
    "description": "The web network.",
    "keywords": [
        "system"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../octo-core",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "a2insights/octo-core": "dev-master",
        "codespb/livewire-notifier": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "joelbutcher/socialstream": "^3.2",
        "laravel-notification-channels/twilio": "^3.1",
        "laravel-views/laravel-views": "^2.2",
        "laravel/cashier": "^13.5",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.4",
        "laravel/nexmo-notification-channel": "^2.5",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.6",
        "livewire/livewire": "^2.7",
        "nexmo/laravel": "^2.4",
        "pusher/pusher-php-server": "~4.0",
        "rappasoft/laravel-livewire-tables": "dev-master",
        "spatie/laravel-enum": "^2.5",
        "spatie/laravel-settings": "^2.3"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.5",
        "laravel-shift/blueprint": "^1.24",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "phpunit/phpunit": "^9.5.8",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": []
    }
}
